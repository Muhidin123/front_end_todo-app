ge = e => {\n    this.setState({\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  render() {\n    return (\n      <div>\n        <Grid\n          textAlign='center'\n          style={{ height: \"50vh\" }}\n          verticalAlign='middle'\n        >\n          <Grid.Column style={{ maxWidth: 450 }}>\n            <Segment stacked>\n              <Form onSubmit={this.handleSubmit} size='large'>\n                <TextArea\n                  value={this.state.title}\n                  onChange={this.handleChange}\n                  name='title'\n                />\n                <TextArea\n                  placeholder=''\n                  value={this.state.description} //ASSIGNEE FROM STATE\n                  onChange={this.handleChange}\n                  name='description'\n                />\n                <Button\n                  color='teal'\n                  fluid\n                  size='large'\n                  type='submit'\n                  onClick={this.handleClick}\n                >\n                  Edit\n                </Button>\n              </Form>\n            </Segment>\n          </Grid.Column>\n        </Grid>\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = state => {\n  return {\n    note: state.notes.notes,\n  };\n};\n\nconst mapDispatchToProps = {\n  updateNote: updateNotesSuccess,\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(SingleCard);\n","/Users/muhidinhukic/Flatiron/code/todo-app/front_end_todo/src/components/DefaultContainer.js",["65"],"import React, { Component } from \"react\";\nimport CardContainer from \"./CardContainer\";\nimport NavBar from \"./NavBar\";\n\nexport default class DefaultContainer extends Component {\n  render() {\n    return (\n      <div>\n        {/* <div>\n          <NavBar />\n        </div> */}\n        <div>\n          <CardContainer />\n        </div>\n      </div>\n    );\n  }\n}\n","/Users/muhidinhukic/Flatiron/code/todo-app/front_end_todo/src/actions/note